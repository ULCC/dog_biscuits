# frozen_string_literal: true
# Generated via
#  `rails generate hyrax:work <%= class_name %>`
# Updated by
#  `rails generate dog_biscuits:work <%= class_name %>`
module Hyrax
  class <%= class_name %>Presenter < Hyrax::WorkShowPresenter
    delegate <% DogBiscuits.config.send("#{file_name}_properties").each { | prop| 
             if class_name.to_s.constantize.controlled_properties.include? prop %>:<%= "#{prop}_label".to_sym %>,<% else %>:<%= prop %>,<% end %>
             <% } %>to: :solr_document
  end
end
